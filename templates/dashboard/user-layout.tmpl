    {{ partial "user-stylesheets" }}
    {{ partial "user-head-scripts" }}
</head>
<body class="fixed-left">
  <!-- Begin page -->
  <div id="wrapper">
    {{ partial "common-dashboard-topbar" }}
    {{ partial "user-left-sidebar" }}
