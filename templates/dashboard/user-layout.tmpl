    <!-- Start right Content here -->
    <!-- ============================================================== -->
    <div class="content-page">
        <!-- Start content -->
        <div class="content">
            <div class="container">
              {{ yield }}
            </div> <!-- container -->
        </div> <!-- content -->
        {{ partial "common-dashboard-footer" }}
    </div>
    <!-- ============================================================== -->
    <!-- End Right content here -->
    <!-- ============================================================== -->
  </div>
  <!-- END wrapper -->

  {{ partial "user-footer-scripts"}}
